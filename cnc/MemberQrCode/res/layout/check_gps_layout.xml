<?xml version="1.0" encoding="UTF-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    
    >
    	<com.google.android.maps.MapView 	
    										android:id="@+id/map_gps"
    										android:layout_width="fill_parent" 
    										android:layout_height="450dp" 
    										android:enabled="true" 
    										android:clickable="true" 
    										android:apiKey="0FDAxYMDVV98TkB4_sBtOpopxgt31J1buUSTSOA" 
    										
    	/>
   
	<LinearLayout 	android:id="@+id/get_message"
					android:orientation="vertical"
    				android:layout_width="fill_parent"
    				android:layout_height="wrap_content"	
    				android:gravity="center_horizontal"
    				android:layout_below="@id/map_gps"
    				
    				android:layout_alignParentBottom="true"
    >
		<ImageButton	android:id="@+id/button_recei_messsage"
						android:layout_x="430px"
						android:layout_y="219px"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:scaleType="center"
						android:background="@drawable/getmess"
						
		/>
		<TextView	android:layout_width="wrap_content"
					android:layout_height="wrap_content" 
					android:text="RECEIVE MESSAGE"
		/>
	</LinearLayout>
	<LinearLayout 	
					android:orientation="vertical"
    				android:layout_width="fill_parent"
    				android:layout_height="wrap_content"	
    				android:gravity="center_horizontal"
    				android:layout_below="@id/get_message"
    				
    >
		<ImageButton	android:id="@+id/button_setting_phone_gps"
						android:layout_x="430px"
						android:layout_y="219px"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:scaleType="center"
						android:background="@drawable/setting"
		/>
		<TextView	android:id="@+id/GetLat"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content" 			
		/>
		<TextView	android:id="@+id/GetLong"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content" 			
		/>
	</LinearLayout>
</RelativeLayout>
<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:background="@drawable/dlall_yes_no"
    android:orientation="vertical" >

    <LinearLayout
        android:id="@+id/LinearLayout01"
        android:layout_width="match_parent"
        android:layout_height="122dip"
        android:layout_weight="122"
        android:orientation="vertical" >

        <LinearLayout
            android:id="@+id/LinearLayout13"
            android:layout_width="match_parent"
            android:layout_height="20dip"
            android:layout_weight="20"
            android:orientation="vertical" >
        </LinearLayout>

        <LinearLayout
            android:id="@+id/LinearLayout12"
            android:layout_width="match_parent"
            android:layout_height="120dip"
            android:layout_weight="120"
            android:orientation="horizontal" android:weightSum="120" android:gravity="center_horizontal">

            <TextView
                android:id="@+id/textView1"
                android:layout_width="100dip"
                android:layout_height="match_parent"

               android:layout_weight="100"/>

        </LinearLayout>

    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout1"
        android:layout_width="match_parent"
        android:layout_height="51dip"
        android:orientation="vertical" android:layout_weight="51">

        <LinearLayout
            android:id="@+id/LinearLayout03"
            android:layout_width="match_parent"
            android:layout_height="51dip"
            android:layout_weight="51"
            android:orientation="horizontal" >

            <LinearLayout
                android:id="@+id/LinearLayout05"
                android:layout_width="136dip"
                android:layout_height="match_parent"
                android:layout_weight="136"
                android:orientation="horizontal" android:weightSum="136" android:gravity="right">

                <LinearLayout
                    android:id="@+id/LinearLayout07"
                    android:layout_width="11dip"
                    android:layout_height="match_parent"
                    android:layout_weight="11"
                    android:orientation="horizontal" >
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/LinearLayout06"
                    android:layout_width="125dip"
                    android:layout_height="match_parent"
                    android:layout_weight="125"
                    android:background="@drawable/btn_one"
                    android:orientation="horizontal" >

                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:id="@+id/LinearLayout04"
                android:layout_width="141dip"
                android:layout_height="match_parent"
                android:layout_weight="141"
                android:orientation="horizontal" >

                <LinearLayout
                    android:id="@+id/LinearLayout09"
                    android:layout_width="200dip"
                    android:layout_height="match_parent"
                    android:layout_weight="200"
                    android:orientation="horizontal" >

                    <LinearLayout
                        android:id="@+id/LinearLayout10"
                        android:layout_width="11dip"
                        android:layout_height="match_parent"
                        android:layout_weight="11"
                        android:orientation="horizontal" >
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/LinearLayout11"
                        android:layout_width="125dip"
                        android:layout_height="match_parent"
                        android:layout_weight="125"
                        android:background="@drawable/btn_one"
                        android:orientation="horizontal" >

                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/LinearLayout08"
                    android:layout_width="9dip"
                    android:layout_height="match_parent"
                    android:layout_weight="9"
                    android:orientation="horizontal" >
                </LinearLayout>
            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:id="@+id/LinearLayout02"
            android:layout_width="match_parent"
            android:layout_height="15dip"
            android:layout_weight="15"
            android:orientation="vertical" >
        </LinearLayout>

    </LinearLayout>

</LinearLayout>
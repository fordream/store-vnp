<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/RelativeLayout1"
    android:layout_width="match_parent"
    android:layout_height="50dp"
    android:background="@drawable/header"
    android:orientation="horizontal" >

    <LinearLayout
        android:id="@+id/linearLayout1"
        android:layout_width="59dip"
        android:layout_height="match_parent" >
    </LinearLayout>

    <LinearLayout
        android:id="@+id/LinearLayout01"
        android:layout_width="59dip"
        android:layout_height="match_parent"
        android:layout_alignParentRight="true" >

    </LinearLayout>

    <org.com.cnc.common.android.views1.ButtonBack
        android:id="@+id/buttonBack1"
        android:layout_width="59dip"
        android:layout_height="35dip"
        android:layout_alignParentLeft="true"
        android:layout_centerVertical="true"/>

    <TextView
        android:id="@+id/textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:layout_toLeftOf="@+id/LinearLayout01"
        android:layout_toRightOf="@+id/linearLayout1"
        android:textAppearance="?android:attr/textAppearanceLarge" android:ellipsize="end" android:gravity="center"/>

</RelativeLayout>
<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:orientation="vertical"
  android:layout_width="fill_parent"
  android:layout_height="fill_parent" android:id="@+id/llContent">
    <RelativeLayout android:layout_height="40dip" android:layout_width="fill_parent" android:background="@drawable/bg" android:id="@+id/relativeLayout1">
        <Button android:layout_alignParentLeft="true" android:layout_marginLeft="22dp" android:id="@+id/btnMenu" android:layout_width="wrap_content" android:layout_alignParentTop="true" android:background="@drawable/btnmenu" android:layout_height="30dip"></Button>
        <RelativeLayout android:layout_width="wrap_content" android:layout_centerVertical="true" android:layout_marginLeft="16dp" android:id="@+id/RelativeLayout1" android:layout_height="wrap_content" android:layout_toRightOf="@+id/btnMenu">
            <Button android:layout_alignParentLeft="true" android:text="200" android:id="@+id/button1" android:textColor="#ffffff" android:layout_width="102dp" android:layout_alignParentTop="true" android:background="@drawable/frame" android:layout_height="30dip"></Button>
            <TextView android:textAppearance="?android:attr/textAppearanceSmall" android:text="scores" android:layout_height="wrap_content" android:layout_alignParentBottom="true" android:layout_centerHorizontal="true" android:layout_width="wrap_content" android:id="@+id/textView1" android:textSize="10dip" android:textStyle="bold" android:typeface="serif"></TextView>
        </RelativeLayout>
        <RelativeLayout android:layout_width="wrap_content" android:layout_alignParentTop="true" android:id="@+id/RelativeLayout01" android:layout_height="wrap_content" android:layout_toRightOf="@+id/RelativeLayout1">
            <Button android:layout_alignParentLeft="true" android:text="1" android:id="@+id/Button01" android:textColor="#ffffff" android:layout_width="102dp" android:layout_alignParentTop="true" android:background="@drawable/frame" android:layout_height="30dip" android:layout_marginLeft="10dip" android:layout_marginRight="10dip"></Button>
            <TextView android:textAppearance="?android:attr/textAppearanceSmall" android:layout_height="wrap_content" android:layout_alignParentBottom="true" android:layout_centerHorizontal="true" android:layout_width="wrap_content" android:id="@+id/TextView01" android:textSize="10dip" android:text="level" android:typeface="serif" android:textStyle="bold"></TextView>
        </RelativeLayout>
        <RelativeLayout android:layout_width="wrap_content" android:id="@+id/RelativeLayout02" android:layout_height="wrap_content" android:layout_alignParentTop="true" android:layout_toRightOf="@+id/RelativeLayout01">
            <Button android:layout_alignParentLeft="true" android:text="23" android:id="@+id/Button02" android:textColor="#ffffff" android:layout_width="102dp" android:layout_alignParentTop="true" android:background="@drawable/frame" android:layout_height="30dip"></Button>
            <TextView android:textAppearance="?android:attr/textAppearanceSmall" android:layout_height="wrap_content" android:layout_alignParentBottom="true" android:layout_centerHorizontal="true" android:layout_width="wrap_content" android:id="@+id/TextView02" android:textSize="10dip" android:typeface="serif" android:textStyle="bold" android:text="life"></TextView>
        </RelativeLayout>
    </RelativeLayout>
    <RelativeLayout android:id="@+id/relativeLayout2" android:layout_width="fill_parent" android:layout_height="fill_parent">
        <LinearLayout android:id="@+id/llContent1" android:layout_width="fill_parent" android:orientation="vertical" android:layout_height="fill_parent"></LinearLayout>
        <ImageView android:id="@+id/imageView1" android:background="@drawable/point" android:layout_alignParentBottom="true" android:layout_height="80dip" android:layout_width="80dip"></ImageView>
        <RelativeLayout android:id="@+id/relativeLayout3" android:layout_alignParentRight="true" android:layout_alignParentBottom="true" android:background="@drawable/btnnewfish" android:layout_width="70dip" android:layout_height="70dip"></RelativeLayout>
    </RelativeLayout>
    
</LinearLayout>
